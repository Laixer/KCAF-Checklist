{
  "name": "kcaf-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "app": "npm run app-kcaf",
    "app-kcaf": "cross-env process.env.VUE_APP_BRAND='kcaf' vue-cli-service serve",
    "app-kaan": "cross-env process.env.VUE_APP_BRAND='kaan' vue-cli-service serve",
    "build": "npm run build-kcaf && npm run build-kaan",
    "build-kcaf": "cross-env process.env.VUE_APP_BRAND='kcaf' vue-cli-service build --dest dist/light",
    "build-kaan": "cross-env process.env.VUE_APP_BRAND='kaan' vue-cli-service build --dest dist/dark",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "aws-sdk": "^2.603.0",
    "core-js": "^2.6.11",
    "jquery": "^3.4.1",
    "module": "^1.2.5",
    "node-sass": "^4.13.0",
    "path": "^0.12.7",
    "sass-loader": "^7.3.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.0.3",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "git-revision-webpack-plugin": "^3.0.4",
    "tar": "^5.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
